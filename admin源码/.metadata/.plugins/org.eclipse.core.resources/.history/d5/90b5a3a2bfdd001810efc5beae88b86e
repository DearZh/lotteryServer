package lottery.domains.content.entity.activity;

public class Common
{
    private double total;
    private double cost;
    
    public double getTotal() {
        return this.total;
    }
    
    public void setTotal(final double total) {
        this.total = total;
    }
    
    public double getCost() {
        return this.cost;
    }
    
    public void setCost(final double cost) {
        this.cost = cost;
    }
}
