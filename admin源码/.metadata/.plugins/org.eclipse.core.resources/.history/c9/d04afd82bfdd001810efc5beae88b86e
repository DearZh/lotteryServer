package lottery.domains.content.vo.chart;

import java.util.ArrayList;
import java.util.List;

public class ChartLineVO
{
    private String[] xAxis;
    private List<Number[]> yAxis;
    
    public ChartLineVO() {
        this.yAxis = new ArrayList<Number[]>();
    }
    
    public String[] getxAxis() {
        return this.xAxis;
    }
    
    public void setxAxis(final String[] xAxis) {
        this.xAxis = xAxis;
    }
    
    public List<Number[]> getyAxis() {
        return this.yAxis;
    }
    
    public void setyAxis(final List<Number[]> yAxis) {
        this.yAxis = yAxis;
    }
}
