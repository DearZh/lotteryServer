package lottery.domains.content.vo.config;

public class GameDividendConfigRule
{
    private double fromLoss;
    private double toLoss;
    private double scale;
    
    public GameDividendConfigRule(final double fromLoss, final double toLoss, final double scale) {
        this.fromLoss = fromLoss;
        this.toLoss = toLoss;
        this.scale = scale;
    }
    
    public double getFromLoss() {
        return this.fromLoss;
    }
    
    public void setFromLoss(final double fromLoss) {
        this.fromLoss = fromLoss;
    }
    
    public double getToLoss() {
        return this.toLoss;
    }
    
    public void setToLoss(final double toLoss) {
        this.toLoss = toLoss;
    }
    
    public double getScale() {
        return this.scale;
    }
    
    public void setScale(final double scale) {
        this.scale = scale;
    }
}
